{
  "expo": {
    "name": "HongPung",
    "slug": "HongPung",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to take photos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select photos.",
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        ""
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "fonts": {
      "NanumSquareNeo-Bold": "./assets/fonts/NanumSquareNeoOTF-Bd.otf",
      "NanumSquareNeo-ExtraBold": "./assets/fonts/NanumSquareNeoOTF-Eb.otf",
      "NanumSquareNeo-Regular": "./assets/fonts/NanumSquareNeoOTF-Rg.otf",
      "NanumSquareNeo-Light": "./assets/fonts/NanumSquareNeoOTF-Lt.otf",
      "NanumSquareNeo-Heavy": "./assets/fonts/NanumSquareNeoOTF-Hv.otf"
    },
    "plugins": [
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true
          },
          "ios": {
            "newArchEnabled": true
          }
        }
      ],
      "expo-secure-store"
    ]
  }
}
